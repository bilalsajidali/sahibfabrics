{
    "version": 2,
    "builds": [
      {
        "src": "SahibFabrics/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxlambaSize": "15mb" , "runtime": "python3.12.0" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config":{
            "distDir":"staticfiles_build"
        }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", 
        "dest": "/static/$1" 
        },
      { "src": "/(.*)", 
      "dest": "SahibFabrics/wsgi.py"
     }
    ]
  }
  